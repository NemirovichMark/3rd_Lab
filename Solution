        for (int i = 0; i < lengh; i++) {
            len[i] = Convert.ToDouble(Console.ReadLine());
            len[i] *= len[i];

        }
        for (int i = 0; i < lengh; i++) { 
            ol+=len[i];
        }
        ol = Math.Sqrt(ol);
        Console.WriteLine(ol);
