            arr39[i] = Convert.ToDouble(Console.ReadLine());
        }
        for (int i = 0; i < n39 - 1; i++)
        {
            cnt391 += 1;
            cnt392 += 1;
            if (arr39[i] >= arr39[i + 1])
            {
                cnt391 = 1;
            }
            if (arr39[i] <= arr39[i + 1])
            {
                cnt391 = 1;
                cnt392 = 1;
            }
            max39 = Math.Max(max39, cnt391);
            max392 = Math.Max(max392, cnt392);
        }
