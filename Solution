                    double k25 = Math.Abs(a25[0]-sr25);
                    int i25 = 0;
                    for(int i=0;i<l25;i++)
                    {
                        if ((Math.Abs(a25[i]-sr25)<k25))
                        {
                            k25 = Math.Abs(a25[i] - sr25);
                            i25 = i;
                        }
                    }
                    for(int i=l25-1;i>i25;i--)
                    {
                        a25[i + 1] = a25[i];
                    }
                    a25[i25 + 1] = p26;
